{
	"name": "Turntable.fm Bot Manager",
	"version": "1.7.2",
	"description": "A bot to keep the mood and manage the dj queue.",
	"background_page": "background.html",
	"content_scripts": [
		{
		"matches": ["http://turntable.fm/*"],
		"js": ["js/jquery-1.6.2.min.js", "js/jquery-ui-1.8.14.min.js", "js/inject.js"]
		}
	],
	"permissions": [
		"tabs",
		"http://turntable.fm/*"
	],
	"icons": { 
		"16":"images/icon-16.png",
		"48":"images/icon-48.png",
		"128":"images/icon-128.png"
	}
}